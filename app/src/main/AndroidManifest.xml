<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jf.simplecustomview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".twosideprogressbar.TwoSideProgressBarActivity"
            android:label="@string/two_side_progress_bar" />
        <activity
            android:name=".temperatureprogress.TemperatureProgressActivity"
            android:label="@string/circle_progress" />
        <activity
            android:name=".mergepicture.MergePictureActivity"
            android:label="@string/merge_picture" />
        <activity
            android:name=".clockview.ClockActivity"
            android:label="@string/clock" />
        <activity android:name=".tickview.TickViewActivity"></activity>
    </application>

</manifest>